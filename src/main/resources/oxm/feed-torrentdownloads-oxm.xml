<?xml version="1.0"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xml-accessor-type="NONE" package-name="io.delimeat.feed.domain"
	xml-mapping-metadata-complete="true">

	<java-types>

		<java-type name="FeedResult">
			<xml-root-element/>
			<java-attributes>
				<xml-element java-attribute="torrentURL" name="info_hash">
					<xml-java-type-adapter value="io.delimeat.feed.jaxb.TorrentDownloadsUrlAdapter" />
				</xml-element>
				<xml-element java-attribute="title" name="title"/>
				<xml-element java-attribute="contentLength" name="size"/>
				<xml-element java-attribute="seeders" name="seeders">
					<xml-java-type-adapter value="io.delimeat.feed.jaxb.ExtraTorrentIntegerAdapter" />
				</xml-element>				
				<xml-element java-attribute="leechers" name="leechers">
					<xml-java-type-adapter value="io.delimeat.feed.jaxb.ExtraTorrentIntegerAdapter" />
				</xml-element>
			</java-attributes>
		</java-type>

		<java-type name="FeedSearch">
			<xml-root-element name="rss"/>
			<java-attributes>
				<xml-element java-attribute="results" xml-path="channel/item"  xml-list="true" />
			</java-attributes>
		</java-type>

	</java-types>

</xml-bindings>