<?xml version="1.0"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xml-accessor-type="NONE" package-name="io.delimeat.core.guide"
	xml-mapping-metadata-complete="true" xml-accessor-order="ALPHABETICAL">

	<java-types>

		<java-type name="GuideSearchResult">
			<xml-root-element/>
			<java-attributes>
				<xml-element java-attribute="description" />
				<xml-element java-attribute="firstAired">
					<xml-java-type-adapter value="io.delimeat.util.jaxb.TvdbDateAdapter" />
				</xml-element>
				<xml-element java-attribute="guideId"/>
				<xml-element java-attribute="title" />
			</java-attributes>
		</java-type>

		<java-type name="GuideEpisode">
			<xml-root-element/>
			<java-attributes>
				<xml-element java-attribute="airDate">
					<xml-java-type-adapter value="io.delimeat.util.jaxb.TvdbDateAdapter" />
				</xml-element>
				<xml-element java-attribute="seasonNum"/>
				<xml-element java-attribute="episodeNum"/>
				<xml-element java-attribute="productionNum"/>
				<xml-element java-attribute="title" />
			</java-attributes>
		</java-type>


		<java-type name="GuideInfo">
			<xml-root-element/>
			<java-attributes>
				<xml-element java-attribute="guideId"/>
				<xml-element java-attribute="firstAired">
					<xml-java-type-adapter value="io.delimeat.util.jaxb.TvdbDateAdapter" />
				</xml-element>
				<xml-element java-attribute="description" />
				<xml-element java-attribute="runningTime" />
				<xml-element java-attribute="timezone" />
				<xml-element java-attribute="title" />
				<xml-element java-attribute="airDays" />
				<xml-element java-attribute="airing" />
				<xml-element java-attribute="airTime">
					<xml-java-type-adapter value="io.delimeat.util.jaxb.AirTimeAdapter" />
				</xml-element>
				<xml-element java-attribute="genres" />
				<xml-element java-attribute="lastUpdated" />
			</java-attributes>
		</java-type>

	</java-types>>

</xml-bindings>